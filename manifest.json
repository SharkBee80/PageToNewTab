{
    "manifest_version": 3,
    "name": "Page To NewTab",
    "version": "1.2.3",
    "description": "A Chrome extension to customize the newtab page.",
    "author": "SharkBee80",
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content_scripts.js"
            ],
            "run_at": "document_start",
            "all_frames": true
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "js/inject.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "action": {
        "default_title": "Page To NewTab",
        "default_popup": "popup/index.html"
    },
    "icons": {
        "16": "./icons/icon-16x16.png",
        "32": "./icons/icon-32x32.png",
        "48": "./icons/icon-48x48.png",
        "64": "./icons/icon-64x64.png",
        "128": "./icons/icon-128x128.png"
    }
}