{
    "manifest_version": 3,
    "name": "Page To NewTab",
    "version": "1.1.1",
    "description": "A Chrome extension to customize the newtab page.",
    "author": "SharkBee80",
    "permissions": ["storage"],
    "host_permissions": [
        "<all_urls>"
    ],
    "chrome_url_overrides": {
        "newtab": "./index.html"
    },
    "action": {
        "default_title": "Page To NewTab",
        "default_popup": "./popup/index.html"
    },
    "icons": {
        "16": "./icon/icon-16x16.png",
        "32": "./icon/icon-32x32.png",
        "48": "./icon/icon-48x48.png",
        "64": "./icon/icon-64x64.png",
        "128": "./icon/icon-128x128.png"
    }
}